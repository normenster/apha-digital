{
  "kind": "collectionType",
  "collectionName": "words",
  "info": {
    "singularName": "word",
    "pluralName": "words",
    "displayName": "Wort",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "kategorie": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "mappedBy": "wort"
    },
    "subkategories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subcategory.subcategory",
      "mappedBy": "worts"
    },
    "silbenanzahl": {
      "type": "enumeration",
      "enum": [
        "a) 1-2",
        "b) 3-4",
        "c) 5-n"
      ],
      "required": true
    },
    "frequency": {
      "type": "enumeration",
      "enum": [
        "hoch",
        "niedrig",
        "hoch bis niedrig"
      ],
      "required": true,
      "default": "hoch"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "nomen",
        "verb",
        "adjektiv"
      ],
      "default": "nomen",
      "required": false
    }
  }
}
